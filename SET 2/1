# Part (a) - Create EmployeeData Dictionary
employee_ids = [101, 102, 103, 104, 105, 106, 107]
salaries = [45000, 52000, 61000, 48000, 39000, 72000, 67000]
employee_data = dict(zip(employee_ids, salaries))

# (a) Find the highest salary without using built-in functions
def find_highest_salary(data):
    highest_salary = -1
    for salary in data.values():
        if salary > highest_salary:
            highest_salary = salary
    return highest_salary

# (b) Count employees earning more than 50,000 without built-in functions
def count_high_salary_employees(data, threshold=50000):
    count = 0
    for salary in data.values():
        if salary > threshold:
            count += 1
    return count

# (c) Find the salary of the employee with ID 106 using linear search
def find_salary_by_id(data, emp_id):
    for key, value in data.items():
        if key == emp_id:
            return value
    return None

# Testing
print("Employee Data:", employee_data)
print("Highest Salary:", find_highest_salary(employee_data))
print("Employees earning more than 50,000:", count_high_salary_employees(employee_data))
print("Salary of Employee with ID 106:", find_salary_by_id(employee_data, 106))
